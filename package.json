{
    "name": "coq-tracer-ui",
    "version": "1.0.0",
    "description": "User interface for the Coq tracer",
    "scripts": {
        "generate-schema": "(echo '/* Automatically generated by `npm run schema`. */'; echo; typescript-json-schema --aliasRefs --topRef --noExtraProps --required --strictNullChecks src/tracer_types.ts Trace) > src/tracer_types_schema.json",
        "build": "parcel build",
        "start": "parcel src/index.html",
        "fmt": "prettier --write .",
        "check": "tsc --noEmit"
    },
    "author": "",
    "license": "ISC",
    "type": "module",
    "devDependencies": {
        "@types/parcel-env": "^0.0.6",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "buffer": "^6.0.3",
        "parcel": "^2.12.0",
        "prettier": "^3.2.5",
        "process": "^0.11.10",
        "typescript": "^5.4.5",
        "typescript-json-schema": "^0.63.0"
    },
    "dependencies": {
        "fzstd": "^0.1.1",
        "ajv": "^8.12.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
